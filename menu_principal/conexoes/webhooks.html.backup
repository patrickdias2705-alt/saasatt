    function copyVariable(variableName) {
      const text = `{{${variableName}}}`;
      navigator.clipboard.writeText(text).then(() => {
        showSuccessMessage('Variável copiada!');
      }).catch(err => {
        console.error('Erro ao copiar:', err);
        alert('Erro ao copiar variável');
