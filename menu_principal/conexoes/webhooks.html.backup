      }
      key += '-';
      
      for (let i = 0; i < 4; i++) {
        key += chars.charAt(Math.floor(Math.random() * chars.length));
      }
      key += '-';
      
      for (let i = 0; i < 4; i++) {
        key += chars.charAt(Math.floor(Math.random() * chars.length));
      }
      key += '-';
      
      for (let i = 0; i < 12; i++) {
