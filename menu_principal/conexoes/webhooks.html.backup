    function getStatusInfo(status) {
      const statusMap = {
        'connected': { class: 'status-connected', text: 'Conectada', canConnect: false, canDisconnect: true },
        'disconnected': { class: 'status-disconnected', text: 'Desconectada', canConnect: true, canDisconnect: false },
        'connecting': { class: 'status-pending', text: 'Conectando', canConnect: false, canDisconnect: false },
        'open': { class: 'status-connected', text: 'Conectada', canConnect: false, canDisconnect: true },
        'close': { class: 'status-disconnected', text: 'Desconectada', canConnect: true, canDisconnect: false },
        'qrReadSuccess': { class: 'status-connected', text: 'Conectada', canConnect: false, canDisconnect: true },
        'qrReadFail': { class: 'status-disconnected', text: 'Falha na Leitura', canConnect: true, canDisconnect: false }
      };

      return statusMap[status] || { class: 'status-disconnected', text: 'Desconhecida', canConnect: true, canDisconnect: false };
    }

    function formatPhoneNumber(phoneNumber) {
      if (!phoneNumber) return 'Número não informado';
      
      const phone = phoneNumber.toString();
      if (phone.length === 13 && phone.startsWith('55')) {
        const area = phone.substring(2, 4);
        const number = phone.substring(4);
        return `+55 (${area}) ${number.substring(0, 5)}-${number.substring(5)}`;
      } else if (phone.length === 12 && phone.startsWith('55')) {
        const area = phone.substring(2, 4);
        const number = phone.substring(4);
        return `+55 (${area}) ${number.substring(0, 4)}-${number.substring(4)}`;
      }
      
      return `+${phone}`;
    }

    function formatDate(dateString) {
      if (!dateString) return 'N/A';
      
      const date = new Date(dateString);
      return date.toLocaleDateString('pt-BR') + ' às ' + date.toLocaleTimeString('pt-BR', { 
        hour: '2-digit', 
        minute: '2-digit' 
      });
    }

    async function disconnectInstance(instanceName, instanceId) {
      if (!confirm(`Tem certeza que deseja desconectar a instância "${instanceName}"?`)) {
        return;
      }

      console.log('Desconectando instância:', { instanceName, instanceId });

      try {
        const instanceData = instancesData.find(inst => inst.id === instanceId);
        
        const webhookData = {
          instanceName: instanceName,
          instanceId: instanceId,
          tenant_id: userData.user.tenant_id,
          instanceData: instanceData,
          action: 'disconnect',
          timestamp: new Date().toISOString(),
          userInfo: {
            userId: userData.user.id,
            userEmail: userData.user.email || userData.user.nome
          }
        };

        console.log('Payload desconectar:', webhookData);

        const response = await fetch('https://n8n-n8n.hgabnb.easypanel.host/webhook/saas-desconectar-instancia-', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify(webhookData)
        });

        if (response.ok) {
          showPopup('success', 'Sucesso!', `Instância "${instanceName}" desconectada com sucesso!`, () => {
            loadInstances();
          });
        } else {
          throw new Error('Erro na resposta do servidor');
        }
      } catch (error) {
        console.error('Erro ao desconectar instância:', error);
        showPopup('error', 'Erro!', 'Erro ao desconectar instância. Tente novamente.');
      }
    }

