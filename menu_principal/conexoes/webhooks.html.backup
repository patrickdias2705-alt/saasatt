    function generateQRCode() {
      const instanceName = document.getElementById('qrInstanceName').value.trim();
      
      if (!instanceName) {
        showPopup('error', 'Atenção!', 'Por favor, digite o nome da instância');
        return;
      }
      
      const generateBtn = document.getElementById('generateButton');
      generateBtn.disabled = true;
      
      const connectForm = document.getElementById('connectForm');
      const loader = document.getElementById('loader');
      
      if (connectForm) connectForm.classList.add('hidden');
      if (loader) loader.classList.remove('hidden');
      
      const webhookData = {
        instanceName: instanceName,
        tenant_id: userData.user.tenant_id
      };
      
      fetch('https://sdr.salesdever.io/webhook/atualizar-qr-code-', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify(webhookData)
      })
      .then(response => {
        if (!response.ok) {
          throw new Error(`HTTP error! status: ${response.status}`);
        }
        const contentType = response.headers.get('content-type');
        if (contentType && contentType.startsWith('image/')) {
          return response.blob();
        } else {
          return response.json();
        }
      })
      .then(data => {
        if (loader) loader.classList.add('hidden');
        const qrcodeDiv = document.getElementById('qrcode');
        if (qrcodeDiv) qrcodeDiv.classList.remove('hidden');
        
        const qrImageDiv = document.getElementById('qrImage');
        if (qrImageDiv) {
          if (data instanceof Blob) {
            const qrCodeUrl = URL.createObjectURL(data);
            qrImageDiv.innerHTML = `
              <div style="width: 280px; height: 280px; background: white; border-radius: 1.5rem; padding: 1.5rem; box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3); display: flex; align-items: center; justify-content: center;">
                <img src="${qrCodeUrl}" alt="QR Code" style="width: 100%; height: 100%; object-fit: contain;" />
              </div>
            `;
          } else if (data.qrCode || data.qr_code) {
            const qrCodeUrl = data.qrCode || data.qr_code;
            qrImageDiv.innerHTML = `
              <div style="width: 280px; height: 280px; background: white; border-radius: 1.5rem; padding: 1.5rem; box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3); display: flex; align-items: center; justify-content: center;">
                <img src="${qrCodeUrl}" alt="QR Code" style="width: 100%; height: 100%; object-fit: contain;" />
              </div>
            `;
          } else {
            qrImageDiv.innerHTML = `
              <div style="width: 280px; height: 280px; background: white; border-radius: 1.5rem; padding: 1.5rem; box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3); display: flex; align-items: center; justify-content: center;">
                <div style="text-align: center;">
                  <div style="font-size: 4rem; margin-bottom: 1rem;">📱</div>
                  <p style="color: #000; font-size: 0.95rem; font-weight: 600;">QR Code para<br>${instanceName}</p>
                </div>
              </div>
            `;
          }
        }
        
        startTimer();
      })
      .catch(error => {
        console.error('Erro:', error);
        
        if (loader) loader.classList.add('hidden');
        if (connectForm) connectForm.classList.remove('hidden');
        
        generateBtn.disabled = false;
        
        showPopup('error', 'Erro!', 'Erro ao gerar QR Code. Tente novamente.');
      });
    }

    let timerInterval = null;
    function startTimer() {
      let timeLeft = 30;
      const timerDiv = document.getElementById('timer');
      const timerText = document.getElementById('timerText');
      const progressPath = document.querySelector('.progress-path');
      
      if (timerDiv) timerDiv.classList.remove('hidden');
      
      const circumference = 2 * Math.PI * 23; // r=23 do círculo
      
      timerInterval = setInterval(() => {
        const minutes = Math.floor(timeLeft / 60);
        const seconds = timeLeft % 60;
        if (timerText) {
          timerText.textContent = `${minutes}:${seconds.toString().padStart(2, '0')}`;
        }
        
        if (progressPath) {
          const progress = timeLeft / 30;
          const offset = circumference * (1 - progress);
          progressPath.style.strokeDasharray = circumference;
          progressPath.style.strokeDashoffset = offset;
        }
        
        if (timeLeft <= 0) {
          clearInterval(timerInterval);
          timerInterval = null;
          
          resetQRCodeDisplay();
          showPopup('error', 'Tempo Esgotado!', 'QR Code expirado. Gere um novo código.');
        }
        
        timeLeft--;
      }, 1000);
    }

    document.getElementById('whatsappNumber').addEventListener('focus', function(e) {
      if (e.target.value === '') {
        e.target.value = '+55 ';
      }
    });

    document.getElementById('whatsappNumber').addEventListener('input', function(e) {
      let value = e.target.value;
      
      if (value === '' || value === '+' || value === '+5') {
        e.target.value = '+55 ';
        return;
      }
      
      let numbers = value.replace(/\D/g, '');
      
      if (!numbers.startsWith('55')) {
        if (numbers.length === 0) {
          e.target.value = '+55 ';
          return;
        }
        numbers = '55' + numbers;
      }
      
      let phoneNumber = numbers.substring(2);
      
      if (phoneNumber.length > 11) {
        phoneNumber = phoneNumber.substring(0, 11);
      }
      
      let formatted = '+55';
      
      if (phoneNumber.length > 0) {
        formatted += ' ' + phoneNumber.substring(0, 2);
        
        if (phoneNumber.length > 2) {
          if (phoneNumber.length <= 6) {
            formatted += ' ' + phoneNumber.substring(2);
          } else if (phoneNumber.length <= 10) {
            formatted += ' ' + phoneNumber.substring(2, 6) + '-' + phoneNumber.substring(6);
          } else {
            formatted += ' ' + phoneNumber.substring(2, 7) + '-' + phoneNumber.substring(7);
          }
        }
      } else {
        formatted += ' ';
      }
      
      e.target.value = formatted;
    });

    document.getElementById('whatsappNumber').addEventListener('keydown', function(e) {
      const cursorPosition = e.target.selectionStart;
      const value = e.target.value;
      
      if ((e.key === 'Backspace' || e.key === 'Delete') && cursorPosition <= 4 && value.startsWith('+55 ')) {
        e.preventDefault();
      }
    });

